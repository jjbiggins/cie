#include "TheBaseClass.h"
#include <iostream>
using namespace std;

TheBaseClass::TheBaseClass(const float fvalue, const int val) : m_float_val{fvalue}, m_val{val}
{
  cout << "In base constructor." << endl;
}

TheBaseClass::~TheBaseClass()
{ cout << "In base destructor." << endl; }

void TheBaseClass::setVal(int val)
{ m_val = val; }

int TheBaseClass::getVal() const
{ return m_val; }

float TheBaseClass::GetFloat_val() const
{
  return m_float_val;
}

